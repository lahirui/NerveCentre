
<!DOCTYPE html>
<html lang="en">

<head>
    <title>NERVE CENTRE</title>

    <!-- Favicon icon -->
    <link rel="icon" href="~/Assets/images/Logo.ico" type="image/x-icon">
    <!-- fontawesome icon -->
    <link rel="stylesheet" href="~/Assets/fonts/fontawesome/css/fontawesome-all.min.css">
    <!-- User icon -->
    <link href="~/Assets/fonts/fontawesome/css/w3.css" rel="stylesheet" />

</head>

<body>
    <div style="overflow-y:hidden;overflow-x:scroll;">
        <!-- [ navigation menu ] start -->
        <nav class="pcoded-navbar icon-colored brand-dark navbar-image-1 title-red active-red">
            <div class="navbar-wrapper">
                <div class="navbar-brand header-logo">
                    <a href="@Url.Action("Index","Home")" class="b-brand">
                        <div>
                            <img  src="~/Assets/images/logo-thumb.png" width="40" height="45" />
                        </div>
                        <span class="b-title">NERVE CENTRE</span>
                    </a>
                    <a class="mobile-menu" id="mobile-collapse" href="javascript:"><span></span></a>
                </div>
                <div class="navbar-content scroll-div">
                    <ul class="nav pcoded-inner-navbar">
                        <li class="nav-item pcoded-menu-caption">
                            <label>MASTER DETAILS</label>
                        </li>
                        <li data-username="dashboard Default Ecommerce CRM Analytics Crypto Project" class="nav-item active">
                            <a href="@Url.Action("Index","Home")" class="nav-link "><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">HOME</span></a>
                        </li>


                       

                        @*
                            <li data-username="FiberMaster" class="nav-item pcoded-hasmenu">
                                <a href="javascript:" class="nav-link "><span class="pcoded-micon"><i class="feather icon-file-text"></i></span><span class="pcoded-mtext">GRN Management</span></a>
                                <ul class="pcoded-submenu">

                                    <li class=""><a href="@Url.Action("Index","GRNs")" class=""><i class="feather icon-activity"></i> Manage GRN</a></li>
                                    <li class=""><a href="@Url.Action("Create","GRNs")" class=""><i class="feather icon-clipboard"></i> Create GRN</a></li>
                                    <li class=""><a href="@Url.Action("Index","ReturnBags")" class=""><i class="feather icon-repeat"></i> Return Bags</a></li>

                                </ul>
                            </li>

                        *@

                       


                        <li class="nav-item pcoded-menu-caption">
                            <label>Log Off</label>
                        </li>
                        <li data-username="Log Off" class="nav-item"><a href="@Url.Action("SignIn","Users")" class="nav-link"><span class="pcoded-micon"><i class="feather icon-power"></i></span><span class="pcoded-mtext">Log Off</span></a></li>

                    </ul>
                </div>
            </div>
        </nav>
        <!-- [ navigation menu ] end -->
        <!-- [ Header ] start -->
        <header class="navbar pcoded-header navbar-expand-lg navbar-light">

                <div class="m-header">
                    <a class="mobile-menu" id="mobile-collapse1" href="javascript:"><span></span></a>
                    <a href="@Url.Action("Index","Home")" class="b-brand">
                        <div>
                            <img src="~/Assets/images/logo-thumb.png" />
                        </div>
                        <span class="b-title">NERVE CENTRE</span>
                    </a>
                </div>
                <a class="mobile-menu" id="mobile-header" href="javascript:">
                    <i class="feather icon-more-horizontal"></i>
                </a>
                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav mr-auto">
                        <li><a href="javascript:" class="full-screen" onclick="javascript:toggleFullScreen()"><i class="feather icon-maximize"></i></a></li>
                        <li class="nav-item">
                            <div class="main-search">
                                <div class="input-group">
                                    <input type="text" id="m-search" class="form-control" placeholder="Search . . .">
                                    <a href="javascript:" class="input-group-append search-close">
                                        <i class="feather icon-x input-group-text"></i>
                                    </a>
                                    <span class="input-group-append search-btn btn btn-primary">
                                        <i class="feather icon-search input-group-text"></i>
                                    </span>
                                </div>
                            </div>
                        </li>
                    </ul>
                    <ul class="navbar-nav ml-auto">
                        <li>
                            <div class="w3-dropdown-hover w3-right">
                                <img src="~/Assets/images/user/UserIcon.png" style="width:40Px; height:40px">
                                <div class="w3-dropdown-content w3-bar-block w3-border shadow-4" style="right:0">
                                    <div class="form-group">
                                        <div class="pro-head" style="background-color:#e9ecef">
                                            <img src="~/Assets/images/user/InnerUser.png" style="width:30Px; height:30px; padding-left:3px">
                                            @*<span><b>HI @Session["LoggedInUserName"].ToString()</b></span>*@
                                        </div>
                                    </div>
                                    <div class="pro-body">
                                        <a href="http://helpgrowlanka.com/" target="_blank" class="dropdown-item"><i class="feather icon-globe"></i> Help Grow Web</a>
                                        @*<a href="@Url.Action("SignOut","Users")" class="dropdown-item"><i class="feather icon-lock"></i> Sign Out </a>*@
                                    </div>
                                </div>
                            </div>

                            <div class="w3-clear"></div>
                        </li>
                    </ul>
                </div>

        </header>
        <!-- [ Header ] end -->
        <!-- [ Main Content ] start -->
        <div class="pcoded-main-container">
            <div class="pcoded-wrapper">
                <div class="pcoded-content">
                    <div class="pcoded-inner-content">
                        <!-- [ breadcrumb ] start -->
                        <!-- [ breadcrumb ] end -->
                        <div class="main-body">
                            <div class="page-wrapper">
                                <!-- [ Main Content ] start -->

                                @RenderBody()

                                <!-- [ Main Content ] end -->
                            </div>
                          
                            <h6 style="color:darkblue; font-family:Arial; font-weight:bold"><b>&copy; @DateTime.Now.Year - NERVE CENTRE</b></h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- [ Main Content ] end -->

        @Scripts.Render("~/Assets/Scripts/Sites")
        @Styles.Render("~/Assets/Styles/Site")
        @RenderSection("scripts", required: false)
    </div>
</body>
</html>